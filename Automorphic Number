#include<stdio.h>
#include<math.h>
int main()
{
    int n,rem,c=0;
    scanf("%d",&n);
    int temp=n;
    int a= n*n;
    while(n>0)
    {
        rem=n%10;
       c++;
       n=n/10;
    }
    int b = pow(10,c);
    c=a%b;
    if(temp==c)
    {
        printf("Automorphic Number");
    }
    else
    {
        printf("Not an Automorphic Number");
    }

}
